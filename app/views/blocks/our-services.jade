.our-services#services
  .grid-fluid
    .inner
      if (isPage === "consulting")
        h2.title!=$i18n.our_services.main_title
          //- span Boost sales
        .list-our-service(data-slide, data-type="ourService", data-toggle="SetHeightItem")
          .item
            .image
              +img('../images/degital-strategy.png', 'Digital Strategy')
            .service-info
              .service-title
                h2.title-1!=$i18n.our_services.consulting.item1.title
              ul(data-set-size="['height']")
                li!=$i18n.our_services.consulting.item1.featured1
                li!=$i18n.our_services.consulting.item1.featured2
                li!=$i18n.our_services.consulting.item1.featured3
                li!=$i18n.our_services.consulting.item1.featured4
                li!=$i18n.our_services.consulting.item1.featured5
          .item
            .image
              +img('../images/degital-design.png', 'Digital Design')
            .service-info
              .service-title
                h2.title-1!=$i18n.our_services.consulting.item2.title
              ul(data-set-size="['height']")
                li!=$i18n.our_services.consulting.item2.featured1
                li!=$i18n.our_services.consulting.item2.featured2
                li!=$i18n.our_services.consulting.item2.featured3
                li!=$i18n.our_services.consulting.item2.featured4
                li!=$i18n.our_services.consulting.item2.featured5
      else
        h2.title!=$i18n.our_services.main_title
          //- span Increase revenue
        .list-our-service(data-slide, data-type="ourService", data-toggle="SetHeightItem")
          .item
            .image
              +img('../images/web-mobile.png', 'Web & Mobile Develoment')
            .service-info
              .service-title
                h2.title-1!=$i18n.our_services.solutions.item1.title
              ul(data-set-size="['height']")
                li!=$i18n.our_services.solutions.item1.featured1
                li!=$i18n.our_services.solutions.item1.featured2
                li!=$i18n.our_services.solutions.item1.featured3
                li!=$i18n.our_services.solutions.item1.featured4
                li!=$i18n.our_services.solutions.item1.featured5
                li!=$i18n.our_services.solutions.item1.featured6
                li!=$i18n.our_services.solutions.item1.featured7
          .item
            .image.image-testing
              +img('../images/testing.png', 'Testing')
            .service-info
              .service-title
                h2.title-1!=$i18n.our_services.solutions.item2.title
              ul(data-set-size="['height']")
                li!=$i18n.our_services.solutions.item2.featured1
                li!=$i18n.our_services.solutions.item2.featured2
                li!=$i18n.our_services.solutions.item2.featured3
                li!=$i18n.our_services.solutions.item2.featured4
                li!=$i18n.our_services.solutions.item2.featured5
                li!=$i18n.our_services.solutions.item2.featured6
           .item
            .image.image-support
              +img('../images/support.png', 'Support')
            .service-info
              .service-title
                h2.title-1!=$i18n.our_services.solutions.item3.title
              ul
                li!=$i18n.our_services.solutions.item3.featured1
                li!=$i18n.our_services.solutions.item3.featured2
                li!=$i18n.our_services.solutions.item3.featured3
                li!=$i18n.our_services.solutions.item3.featured4
